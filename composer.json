{
	"name": "flarum-lang/ukrainian",
	"type": "flarum-extension",
	"description": "Ukrainian language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/ukrainian",
	"keywords": [
		"locale",
		"flarum",
		"uk-locale"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Denys Nosov",
			"email": "denys@joomla-ua.org",
			"homepage": "https://community.joomla-ua.org/"
		}
	],
	"support": {
		"issues": "https://github.com/flarum-lang/ukrainian/issues",
		"source": "https://github.com/flarum-lang/ukrainian"
	},
	"require": {
		"flarum/core": "^1.3"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"branch-alias": {
			"dev-master": "1.x-dev"
		},
		"flarum-extension": {
			"title": "Українська",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "uk",
			"title": "Українська"
		},
		"extiverse": {
			"discuss": "https://discuss.flarum.org/d/31921"
		}
	}
}
